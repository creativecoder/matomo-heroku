{
    "name": "creativecoder/dokku-piwik",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "matomo/matomo-icons",
                "type": "piwik-plugin",
                "version": "0.0.4",
                "dist": {
                    "url": "https://github.com/matomo-org/matomo-icons/archive/master.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                },
                "extra": {
                    "installer-name": "icons"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "matomo/matomo-log-analytics",
                "type": "piwik-plugin",
                "version": "0.0.3",
                "dist": {
                    "url": "https://github.com/matomo-org/matomo-log-analytics/archive/master.zip",
                    "type": "zip"
                },
                "extra": {
                    "installer-name": "log-analytics"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "matomo/plugin-SecurityInfo",
                "version": "3.0.6",
                "dist": {
                    "url": "https://github.com/matomo-org/plugin-SecurityInfo/archive/3.0.6.zip",
                    "type": "zip"
                },
                "type": "piwik-plugin",
                "require": {
                    "composer/installers": "~1.0"
                },
                "extra": {
                    "installer-name": "SecurityInfo"
                }
            }
        }
    ],
    "require": {
        "piwik/piwik": "^3.8",
        "php": "^7.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-xml": "*",
        "ext-mbstring": "*",
        "matomo/plugin-SecurityInfo": "^3.0",
        "geoip2/geoip2": "^2.9",
        "matomo/matomo-icons": "^0.0.4",
        "matomo/matomo-log-analytics": "^0.0.3",
        "composer/installers": "^1.7"
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Grant Kinney",
            "email": "hi@grant.mk"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": "cp -R plugins/* vendor/piwik/piwik/plugins/ && cp -R misc/* vendor/piwik/piwik/misc/ && cp -R icons/* vendor/piwik/piwik/plugins/Morpheus/icons/ && cp index.php vendor/piwik/piwik/ && cp config.ini.php vendor/piwik/piwik/config/ && if [ -d .heroku ]; then cp .geoip/share/GeoLite2-City.mmdb .geoip/share/GeoLite2-Country.mmdb vendor/piwik/piwik/misc/; fi"
    },
    "extra": {
        "installer-paths": {
            "{$name}/": [
                "matomo/matomo-icons"
            ],
            "misc/{$name}": [
                "matomo/matomo-log-analytics"
            ]
        }
    }
}
